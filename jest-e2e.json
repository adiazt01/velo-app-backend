{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testPathIgnorePatterns": ["/node_modules/", "/dist/", "/generated/"],
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "coverageDirectory": "coverage-e2e",
  "modulePaths": ["<rootDir>"]
}
